<h1> this is my first contact form</h1>
<%= form_tag contact_path do %>
<div>
<%= label_tag :name %>
<%= text_field_tag :name %>
</div>
<div>
<%= label_tag :email %>
<%= text_field_tag :email %>
</div>
<div>
<%= label_tag :massage %>
<%= text_area_tag :massage %>
</div>
<%= submit_tag "submit!" %>
<% end %>
